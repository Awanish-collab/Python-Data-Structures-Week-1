fname = input("Enter file name: ")
fh = open(fname)
count = 0
for line in fh:
    if line.startswith('From:'):
        continue
    elif line.startswith('From'):
        line = line.split()
        line = line[1]
        print(line)
    else:
        continue
    count += 1
